<% content_for :title, "Editing facebook sync" %>

<div class="card">
  <div class="card-header">
    <h1 class="mb-0">Editing facebook sync</h1>
  </div>
  <div class="card-body">
    <%= render "form", facebook_sync: @facebook_sync %>

    <div class="mt-3">
      <div class="btn-group">
        <%= link_to "Show this facebook sync", @facebook_sync, class: "btn btn-outline-primary" %>
        <%= link_to "Back to facebook syncs", facebook_syncs_path, class: "btn btn-outline-secondary" %>
      </div>
    </div>
  </div>
</div>
