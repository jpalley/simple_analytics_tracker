<p style="color: green"><%= notice %></p>

<% content_for :title, "Facebook syncs" %>

<h1>Facebook syncs</h1>

<div id="facebook_syncs">
  <table class="table table-striped">
 app/views/facebook_syncs/index.html.erb
// ... existing code ...
    <thead>
      <tr>
        <th>Created at</th>
        <th>Updated at</th>
        <th>Enabled</th>
        <th>Test mode</th>
        <th>Table name</th>
        <th>Event name</th>
        <th>Event value</th>
        <th>Last counter</th>
        <th>Event source url</th>
      </tr>
    </thead>
    <% @facebook_syncs.each do |facebook_sync| %>
      <tr>
        <td><%= facebook_sync.created_at %></td>
        <td><%= facebook_sync.updated_at %></td>
        <td><%= facebook_sync.enabled %></td>
        <td><%= facebook_sync.test_mode %></td>
        <td><%= facebook_sync.table_name %></td>
        <td><%= facebook_sync.event_name %></td>
        <td><%= facebook_sync.event_value %></td>
        <td><%= facebook_sync.last_counter %></td>
        <td><%= facebook_sync.event_source_url %></td>
        <td><%= link_to "Show", facebook_sync %></td>
        <td><%= link_to "Edit", edit_facebook_sync_path(facebook_sync) %></td>
        <td><%= link_to "Destroy", facebook_sync, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

<%= link_to "New facebook sync", new_facebook_sync_path %>
