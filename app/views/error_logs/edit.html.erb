<% content_for :title, "Editing error log" %>

<div class="card">
  <div class="card-header">
    <h1 class="mb-0">Editing error log</h1>
  </div>
  <div class="card-body">
    <%= render "form", error_log: @error_log %>

    <div class="mt-3">
      <div class="btn-group">
        <%= link_to "Show this error log", @error_log, class: "btn btn-outline-primary" %>
        <%= link_to "Back to error logs", error_logs_path, class: "btn btn-outline-secondary" %>
      </div>
    </div>
  </div>
</div>
